    <fragment
        android:id="@+id/navigation_academics"
        android:name="com.cheise_proj.core.feature.student.ui.academics.AcademicsFragment"
        android:label="fragment_academics"
        tools:layout="@layout/fragment_academics" />
    <fragment
        android:id="@+id/navigation_account"
        android:name="com.cheise_proj.core.feature.student.ui.account.AccountMainFragment"
